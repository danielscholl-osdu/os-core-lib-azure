<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
        "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
    <suppress checks="LineLengthCheck"
              files="."/>
    <suppress checks="MagicNumber"
              files="."/>
    <suppress checks="ParameterNumber"
              files="."/>
    <suppress checks="ParameterName"
              files="."/>
    <!-- No need to have java docs for private variables -->
    <suppress checks="JavadocVariable"
              files="."/>
    <!-- This check will always fail on windows, so we ignore it -->
    <suppress checks="NewlineAtEndOfFile"
              files="."/>
    <!-- So we don't need package-info.java files -->
    <suppress checks="JavadocPackage"
              files="."/>
</suppressions>